{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "module": "Node16",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2015",
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["DOM", "ES2020"]
  },
  "include": ["./"]
}
