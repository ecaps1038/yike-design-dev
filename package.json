{
  "name": "yike-design-dev",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "pnpm build:prepare && pnpm gen:icon",
    "dev:demo": "pnpm -F demo dev",
    "preview:demo": "pnpm -F demo preview",
    "build": "pnpm -F @yike/build build",
    "build:comp": "pnpm -F @yike/build build:comp",
    "build:prepare": "pnpm build:utils && pnpm build:scripts",
    "build:utils": "pnpm -F @yike/build-utils build",
    "build:scripts": "pnpm -F @yike/build-scripts build",
    "gen:icon": "node internal/scripts/dist/index.js genicon",
    "new:comp": "node internal/scripts/dist/index.js newcomp",
    "test": "pnpm -F @yike/test test",
    "test:coverage": "pnpm -F @yike/test test:coverage",
    "eslint": "npx eslint --ext .ts,.tsx,.js,.jsx,.vue . --fix",
    "stylelint": "npx stylelint --aei .vue,.less,.css,.html . --fix",
    "lint": "eslint . && stylelint .",
    "lint:fix": "pnpm run eslint && pnpm run stylelint",
    "postinstall": "npx husky install",
    "preinstall": "npx only-allow pnpm",
    "release": "standard-version"
  },
  "dependencies": {
    "@yike/components": "workspace: *",
    "@yike/styles": "workspace: *",
    "@yike/utils": "workspace: *"
  },
  "devDependencies": {
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@yike/build": "workspace: *",
    "@yike/build-scripts": "workspace: *",
    "@yike/build-utils": "workspace: *",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.0",
    "less": "^4.0.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-less": "^1.0.8",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,less,vue}": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}
